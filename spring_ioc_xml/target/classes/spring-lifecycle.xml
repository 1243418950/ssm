<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="user" class="com.atguigu.spring.pojo.User" init-method="initMethod" destroy-method="destroyMethod" >
        <property name="id" value="222"></property>
        <property name="username" value="客户"></property>
    </bean>
    <bean id="mybeanprocess" class="com.atguigu.spring.process.MyBeanPostProcess"></bean>
</beans>